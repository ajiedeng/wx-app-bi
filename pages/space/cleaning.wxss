.label {
    font-size: 32rpx;
    font-weight: 400;
    color: rgba(17, 17, 17, 1);
    line-height: 44rpx;
}

.lastRecord {
    display: flex;
    border-bottom: 2rpx solid rgba(225, 225, 225, 1);
    padding: 38rpx 0;
}

.time {
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 40rpx;
    flex: 1;
    text-align: right;
}

.cleaning {
    background: rgba(255, 255, 255, 1);
    padding: 0 32rpx 36rpx 32rpx;
    margin-top: 24rpx;
}

.addImgLabel{
    padding: 36rpx 0;
}


.circleBox {
    width: 304rpx;
    height: 304rpx;
    margin: 70rpx auto;
    background-color: rgba(0,127,255,.15);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.circleBox .circleInner{
    display: flex;
    flex-direction: column;
    margin: auto;
    width: 272rpx;
    height: 272rpx;
    background:linear-gradient(183deg,rgba(36,182,255,1) 0%,rgba(0,127,255,1) 100%);
    border-radius: 50%;
    align-items: center;
    justify-content: center;
}
.circleBox.late{
    background-color: rgba(255,140,22,.15);
}
.circleBox.late .circleInner{
    background:linear-gradient(183deg,#f1bf8c 0%,#FF8C16 100%);
}
.circleBox .circleInner .circleTxt{
    font-size: 40rpx;
    color: #fff;
    font-weight: 500;
}
.circleBox .circleInner .curTime{
    font-size: 28rpx;
    color: rgba(255,255,255,.6);
    font-weight: 400;
}