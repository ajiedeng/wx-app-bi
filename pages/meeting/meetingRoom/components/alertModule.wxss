/* pages/mettingRoom/components/alertModule.wxss */
.toast{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #000000;
  opacity: .6;
  z-index: 1;
}
.content{
  width: 288px;
  border-radius: 4px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: #FFFFFF;
  z-index: 2;
}