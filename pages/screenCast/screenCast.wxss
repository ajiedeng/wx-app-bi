.videShow {
    width: 250rpx;
    height: 250rpx;
    border-radius: 50%;
    background: #0E94FF;
    position: relative;
}

.videIcon {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.videIcon text {
    display: block;
    font-size: 32rpx;
    color: #fff;
}

.occupyByOther {
    width: 100%;
    text-align: center;
    height: 100%;
    position: fixed;
    left: 0;
    top:0;
    background-color: #efeff4;
    z-index: 99;
}

.occupyByOther view {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    display: flex;
    flex-direction: column;
}

.occupyByOther image {
    width: 360rpx;
    height: 360rpx;
    margin: 0 auto;
    justify-content: space-evenly;
}

.occupyByOther text {
    font-size: 28rpx;
    color: #999999;
}
.occupyByOther p {
    font-size: 16px;
    margin: 0 auto;
    justify-content: space-evenly;
    color: #1F8EFF;
}
.tabContainer{
    display: flex;
    width:100%;
    justify-content: space-between;
}
.image .tabContainer{
    justify-content: space-evenly;
}
.playContainer van-tabs{
    padding:0 96rpx;
    width: 100%;
    box-sizing: border-box;
}
.playContainer van-tab{
   display: block;
   margin-top:64rpx;
}
.icon-time::before{
    content: "";
    display: block;
    width: 56rpx;
    height: 56rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}
.icon-paly:before {
    content: "";
    display: block;
    width: 26rpx;
    height: 32rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}
.icon-pause:before {
    content: "";
    display: block;
    width: 24rpx;
    height: 34rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}
.icon-change::before{
    content: "";
    display: block;
    width: 52rpx;
    height: 44rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}

.title-defind .van-tabs__wrap{
    width: 392rpx;
    margin: 0 auto;
}
.van-tabs__nav--card .van-tab:first-child.van-tab--active {
    color: #0F86DD;
    background-color: #fff!important;
    border: var(--border-width-base,1px) solid var(--tabs-default-color,#0F86DD)!important;
    border-right: var(--border-width-base,1px) solid var(--tabs-default-color,#D9D9D9)!important;
}
.title-defind .van-tabs__nav--card .van-tab:last-child.tab-css{
    background-color: #fff!important;
    border: var(--border-width-base,1px) solid var(--tabs-default-color,#0F86DD)!important;
    border-left: var(--border-width-base,1px) solid var(--tabs-default-color,#D9D9D9)!important;
    border-right: var(--border-width-base,1px) solid var(--tabs-default-color,#0F86DD)!important;
}
.title-defind .van-tabs__nav--card{
    border:none;
}
.title-defind .van-tab {
    border: var(--border-width-base,1px) solid var(--tabs-default-color,#D9D9D9);
}
.title-defind .van-tab:first-child{
    border-right: none;
}
.title-defind .van-tab:last-child{
    border-left: none;
}
.title-defind .van-tabs__nav--card .van-tab:last-child {
    border-right: var(--border-width-base,1px) solid var(--tabs-default-color,#D9D9D9)!important;
}
