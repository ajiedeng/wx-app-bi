.page {
    display: flex;
    flex-direction: column;
    height: 100%;
    width: 100%;
}


.playContainer {
    height: 400rpx;
    background: white;
    display: flex;
    align-items: center;
    justify-content: space-around;

}
.playContainer.image{
    align-items: initial;
    padding-top:48rpx;
    box-sizing: border-box;
}

.btnIcon .icon {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background: #F7F7F7;
    text-align: center;
    margin-bottom: 24rpx;
}

.btnIcon:first-child .weui-icon {
    transform: rotateY(-180deg);
}

.btnIcon .weui-icon {
    position: relative;
    top: 50%;
    margin-top: -60rpx;
}

.btnIcon van-icon {
    height: 100%;
}

.btnIcon text {
    font-size: 28rpx;
    color: #999;
}

.bottomPanel {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
    /* height: 672rpx; */
}

.bottomPanel .overScreen {
    height: 124rpx;
    line-height: 124rpx;
    background: white;
    border-top: 1rpx solid #E1E1E1;
}

.overScreen text {
    font-size: 32rpx;
    color: #007FFF;
}

.icon .my-icon {
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

